{
    "version": 2,
    "builds": [
        {
            "src": "api/checkout.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/api/create-checkout-session",
            "dest": "api/checkout.js"
        },
        {
            "src": "/(.*)",
            "dest": "/public/$1"
        }
    ]
}